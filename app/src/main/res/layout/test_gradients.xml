<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="From_PNG"
        tools:layout_editor_absoluteX="124dp"
        tools:layout_editor_absoluteY="443dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="From_Shape"
        tools:layout_editor_absoluteX="231dp"
        tools:layout_editor_absoluteY="174dp" />

    <ImageView
        android:id="@+id/imageBanner"
        android:layout_width="114dp"
        android:layout_height="187dp"
        android:layout_marginTop="212dp"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toStartOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.242"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="#FFFF0000"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageMask"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/ic_mask_from_svg"
        android:scaleType="fitXY"

        app:layout_constraintBottom_toBottomOf="@+id/imageBanner"
        app:layout_constraintEnd_toEndOf="@+id/imageBanner"
        app:layout_constraintStart_toStartOf="@+id/imageBanner"
        app:layout_constraintTop_toTopOf="@+id/imageBanner"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="128dp"
        android:layout_height="0dp"
        android:layout_marginEnd="52dp"
        android:background="#FFFF0000"
        app:layout_constraintBottom_toBottomOf="@+id/imageBanner"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageBanner"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="128dp"
        android:layout_height="187dp"
        android:background="#FFFF0000"
        tools:ignore="ContentDescription"
        tools:layout_editor_absoluteX="124dp"
        tools:layout_editor_absoluteY="476dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:srcCompat="@drawable/mask" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="From_SVG_Figma"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="174dp" />
    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7"
        app:srcCompat="@drawable/mask_png" />

</androidx.constraintlayout.widget.ConstraintLayout>